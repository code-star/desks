{
  "compilerOptions": {
    "target": "es2019",
    "lib": ["dom", "dom.iterable", "es2019"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "declaration": true,
    "sourceMap": true,
    "baseUrl": "./",
    "rootDir": "./",
    "noEmit": false
  }
}
